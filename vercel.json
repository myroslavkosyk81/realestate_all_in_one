{
  "name": "express-static-website",
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "/api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    
    { "src": "/client/(.*)", "dest": "/client/index.html"}
  ]

   

}